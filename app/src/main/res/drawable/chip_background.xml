<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/chips_radius"/>
            <solid android:color="@color/colorAccentLight"/>
        </shape>
    </item>
    <item
        android:top="@dimen/chips_border"
        android:left="@dimen/chips_border"
        android:right="@dimen/chips_border"
        android:bottom="@dimen/chips_border">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/chips_radius"/>
            <solid android:color="@color/white_soft_cyan"/>
        </shape>
    </item>

</layer-list>